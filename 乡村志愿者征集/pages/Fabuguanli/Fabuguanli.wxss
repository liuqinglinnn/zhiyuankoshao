  margin-left: 20rpx;
}
.team .team_ .team_for {
  width: 90%;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
  margin-left: 5%;
  border-top: 2rpx solid #b7b7b7;
  border-bottom: 2rpx solid #b7b7b7;
}
.team .team_ .team_for .team_text {
  font-size: 30rpx;
  margin-left: 25rpx;
  opacity: 0.7;
  width: 140rpx;
}
.team .team_ .team_for .team_img {
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  position: relative;
  width: 250rpx;
  height: 70rpx;
}
.team .team_ .team_for .team_img .team_forimg {
  position: relative;
  width: 50rpx;
  box-sizing: border-box;
  transform: translateX(50rpx);
  margin-top: -5rpx;
}
.team .team_ .team_for .team_img .team_forimg .team_forimg_ {
  background-color: antiquewhite;
  border-radius: 50%;
  overflow: hidden;
  width: 70rpx;
  height: 70rpx;
  border: 2rpx solid #b7b7b7;
}
.team .team_ .team_for .team_img .team_forimgother {
  position: relative;
  width: 70rpx;
  height: 70rpx;
  transform: translateX(50rpx);
  background-color: antiquewhite;
  border-radius: 50%;
  overflow: hidden;
  border: 2rpx solid #b7b7b7;
  margin-top: -5rpx;
}
.team .team_ .team_for .teambuttom {
  height: 75rpx;
  position: absolute;
  right: 50rpx;
  width: 175rpx;
  border-radius: 50rpx;
  background-color: #3ab54a;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: white;
}
.team .addnew {
  width: 90%;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
