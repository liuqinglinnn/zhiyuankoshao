.team {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: white;
  padding-bottom: 150rpx;
}
.team .t {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 100rpx;
  background-color: white;
}
.team .t .timg {
  width: 50rpx;
  height: 50rpx;
