  font-size: 30rpx;
  margin-left: 25rpx;
  opacity: 0.7;
  width: 140rpx;
}
.team .addnew .addnew_img {
  display: flex;
  flex-direction: row;
  position: relative;
  transform: translateX(50rpx);
  background-color: antiquewhite;
  border-radius: 50%;
  overflow: hidden;
  width: 70rpx;
  height: 70rpx;
}
.team .addnew .addnew_img .addnew_imgone {
  width: 70rpx;
  height: 70rpx;
}
.team .addnew .addbuttom {
  height: 75rpx;
  position: absolute;
  right: 50rpx;
  width: 175rpx;
  border-radius: 50rpx;
  background-color: #3ab54a;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: white;
}
.buttom {
  position: fixed;
  bottom: 0rpx;
  height: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: white;
  align-items: center;
  border-top: 2rpx solid #b7b7b7;
}
.buttom .one {
  width: 70%;
  height: 100rpx;
  text-align: center;
  border-right: 2rpx solid #b7b7b7;
  display: flex;
  justify-content: center;
  align-items: center;
}
.buttom .one .onetext {
  width: 400rpx;
  height: 75rpx;
  border: 5rpx solid #3ab54a;
  background-color: #3ab54a;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  color: white;
}
.buttom .two {
  width: 30%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-left: 2rpx solid #b7b7b7;
}
.buttom .two .twotext {
  width: 150rpx;
  height: 75rpx;
  border: 5rpx solid #6c99be;
  background-color: #6c99be;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  color: white;
}
